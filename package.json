{
  "name": "DarkSkyClient",
  "version": "0.0.1",
  "devDependencies": {
    "app-root-path": "^1.0.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "2.7.12",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "flux": "^2.0.3",
    "future-loader": "^1.0.3",
    "jsx-loader": "^0.13.2",
    "lint-staged": "^3.2.1",
    "node-libs-browser": "^0.5.2",
    "object-assign": "^3.0.0",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "future-router": "^1.0.2",
    "moment": "^2.17.0"
  },
  "scripts": {
    "eslint": " eslint --config .eslintrc.json --fix . --ext .jsx "
  },
  "pre-commit": "eslint",
  "lint-staged": {
    "eslint": "*.jsx",
    "stylelint": "*.css"
  },
  "paths": {
    "app": {
      "all": "/app",
      "main": "main.js"
    },
    "public": {
      "js": {
